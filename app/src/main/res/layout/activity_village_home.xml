<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VillageHomeActivity">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_left_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="15dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_right_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="15dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="80dp" />


    <ImageView
        android:id="@+id/close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/close_icon" />


    <TextView
        android:id="@+id/VillageName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="Village Name"
        android:textSize="@dimen/_20sdp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintTop_toBottomOf="@+id/close" />

    <TextView
        android:id="@+id/StateCountry"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:gravity="center"
        android:text="Province,Country"
        android:textSize="@dimen/_15sdp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintTop_toBottomOf="@+id/VillageName" />

    <com.android.volley.toolbox.NetworkImageView
        android:id="@+id/home_image"
        android:layout_width="0dp"
        android:layout_height="230dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/StateCountry" />

    <View
        android:id="@+id/line01"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:background="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintTop_toBottomOf="@+id/home_image" />

    <TextView
        android:id="@+id/history"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="History"
        android:textSize="@dimen/_18sdp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline_center"
        app:layout_constraintTop_toBottomOf="@+id/line01" />

    <View
        android:id="@+id/line02"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintTop_toBottomOf="@+id/history" />


            <ImageView
                android:id="@+id/history_led"
                android:layout_width="31dp"
                android:layout_height="23dp"
                android:layout_marginEnd="4dp"
                app:layout_constraintBottom_toBottomOf="@+id/line02"
                app:layout_constraintEnd_toStartOf="@+id/guideline_center"
                app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
                app:layout_constraintTop_toBottomOf="@+id/line01"
                app:srcCompat="@drawable/grey_led" />
    <TextView
        android:id="@+id/chieftainship"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Chieftainship"
        android:textSize="@dimen/_18sdp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline_center"
        app:layout_constraintTop_toBottomOf="@+id/line02" />
    <View
        android:id="@+id/line03"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintTop_toBottomOf="@+id/chieftainship" />
    <ImageView
        android:id="@+id/chieftainship_led"
        android:layout_width="31dp"
        android:layout_height="23dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_center"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintBottom_toBottomOf="@+id/line03"
        app:layout_constraintTop_toBottomOf="@+id/line02"
        app:srcCompat="@drawable/grey_led"
        />
    <TextView
        android:id="@+id/tribe"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Tribe"
        android:textSize="@dimen/_18sdp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline_center"
        app:layout_constraintTop_toBottomOf="@+id/line03" />
    <View
        android:id="@+id/line04"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintTop_toBottomOf="@+id/tribe" />
    <ImageView
        android:id="@+id/tribe_led"
        android:layout_width="31dp"
        android:layout_height="23dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline_center"
        app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
        app:layout_constraintBottom_toBottomOf="@+id/line04"
        app:layout_constraintTop_toBottomOf="@+id/line03"
        app:srcCompat="@drawable/grey_led"
        />
    <TextView
        android:id="@+id/language"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
