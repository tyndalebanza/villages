                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="Architecture"
                android:textSize="@dimen/_18sdp"
                app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline_center"
                app:layout_constraintTop_toBottomOf="@+id/line08" />
            <View
                android:id="@+id/line09"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="@color/black"
                app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
                app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
                app:layout_constraintTop_toBottomOf="@+id/architecture" />
            <ImageView
                android:id="@+id/architecture_led"
                android:layout_width="31dp"
                android:layout_height="23dp"
                android:layout_marginEnd="4dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline_center"
                app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
                app:layout_constraintBottom_toBottomOf="@+id/line09"
                app:layout_constraintTop_toBottomOf="@+id/line08"
                app:srcCompat="@drawable/grey_led"
                />
            <TextView
                android:id="@+id/dressing"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="Dressing"
                android:textSize="@dimen/_18sdp"
                app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline_center"
                app:layout_constraintTop_toBottomOf="@+id/line09" />
            <View
                android:id="@+id/line10"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="@color/black"
                app:layout_constraintEnd_toStartOf="@+id/guideline_right_01"
                app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
                app:layout_constraintTop_toBottomOf="@+id/dressing" />
            <ImageView
                android:id="@+id/dressing_led"
                android:layout_width="31dp"
                android:layout_height="23dp"
                android:layout_marginEnd="4dp"
                app:layout_constraintEnd_toStartOf="@+id/guideline_center"
                app:layout_constraintStart_toStartOf="@+id/guideline_left_01"
                app:layout_constraintBottom_toBottomOf="@+id/line10"
                app:layout_constraintTop_toBottomOf="@+id/line09"
                app:srcCompat="@drawable/grey_led"
                />
